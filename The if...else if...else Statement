let temperature = 15;

if (temperature > 30) {
    console.log("It's a very hot day! Stay indoors.");
} else if (temperature > 20) {
    console.log("It's warm outside. Enjoy your day!");
} else if (temperature > 10) {
    console.log("It's a bit chilly. Wear a jacket.");
} else {
    console.log("It's cold outside. Stay warm!");
}
